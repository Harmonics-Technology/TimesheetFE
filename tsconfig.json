{
    "compilerOptions": {
            "target": "es5",
            "lib": ["dom", "dom.iterable", "esnext"],
            "allowJs": true,
            "skipLibCheck": true,
            "strict": true,
            "forceConsistentCasingInFileNames": true,
            "noEmit": true,
            "esModuleInterop": true,
            "module": "esnext",
            "moduleResolution": "node",
            "resolveJsonModule": true,
            "isolatedModules": true,
            "noImplicitAny": false,
            "jsx": "preserve",
        "baseUrl": ".",
        "paths": {
            "@definitions/*": ["src/definitions/*"],
            "@definitions": ["src/definitions"],
            "@components/*": ["src/components/*"],
            "@components": ["src/components"],
            "@public/*": ["public/*"],
            "@public": ["public"],
            "@interfaces/*": ["src/interfaces/*"],
            "@interfaces": ["src/interfaces"],
            "@styles/*": ["src/styles/*"],
            "@styles": ["src/styles"],
            "@components/icons/*": ["src/components/icons/*"],
            "@components/icons": ["src/components/icons"],
            "@mobx/*": ["src/mobx/*"],
            "@mobx": ["src/mobx"],
            "@i18n": ["./i18n"],
            "@test/*": ["test/*"],
            "@test": ["test"]
        },
        "incremental": true
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "src/components/subpages",
        "pages/TeamMember/dashboard"
, "src/components/generics/useLeavePageConfirmation.js", "src/components/generics/withDnDContext.js", "pages/SuperAdmin/account-management", "src/asseccibility.js"    ],
    "exclude": ["node_modules", "test/*"]
}
