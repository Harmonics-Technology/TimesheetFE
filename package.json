{
    "name": "timesheet",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build  && rm -rf ./.next/cache",
        "start": "next start",
        "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "test": "jest -c test/jest.config.js",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
        "schema:fetch": "node ./script/fetch-schema.js",
        "schema:hooks": "openapi --input ./src/types/api-schema.json --output ./src/types",
        "schema:generate": "node ./script/schema-codegen.js --srcPath ./src/types/api-schema.json --destPath ./src/types/api --separatedFiles true",
        "schema:codegen": "yarn schema:fetch && yarn schema:generate",
        "format-staged": "pretty-quick --staged",
        "openapi:codegen": "openapi --input https://insighttimesheetapi.azurewebsites.net/swagger/v1/swagger.json --output ./src/services --client axios"
    },
    "dependencies": {
        "@chakra-ui/icons": "^2.0.15",
        "@chakra-ui/react": "^2.4.3",
        "@codegena/oapi3ts-cli": "^2.1.10-alpha.8",
        "@emotion/react": "^11.1.4",
        "@emotion/styled": "^11.0.0",
        "@hookform/resolvers": "^2.9.10",
        "@next/bundle-analyzer": "^13.1.1",
        "@progress/kendo-drawing": "^1.17.2",
        "@progress/kendo-licensing": "^1.3.0",
        "@progress/kendo-react-pdf": "^5.9.0",
        "@types/js-cookie": "^3.0.2",
        "@uploadcare/react-widget": "^2.4.0",
        "axios": "^0.21.1",
        "cross-env": "^7.0.3",
        "date-fn": "^0.0.2",
        "date-fns": "^2.29.3",
        "framer-motion": "^3.2.1",
        "js-cookie": "^3.0.1",
        "js-file-download": "^0.4.12",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.5.28",
        "mobx": "^6.0.4",
        "mobx-react": "^7.0.5",
        "mobx-state-tree": "^5.0.1",
        "moment": "^2.29.4",
        "next": "^12.1.6",
        "next-compose-plugins": "^2.2.1",
        "next-i18next": "^7.0.1",
        "nextjs-progressbar": "^0.0.16",
        "num-words": "^1.2.2",
        "openapi-typescript-codegen": "^0.23.0",
        "prettier": "^2.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.39.5",
        "react-icons": "^4.7.1",
        "react-loading-skeleton": "^3.2.0",
        "react-month-year-picker": "^1.0.1",
        "react-multi-date-picker": "^3.3.4",
        "react-phone-input-2": "^2.15.1",
        "react-phone-number-input": "^3.2.13",
        "react-query": "^3.5.16",
        "react-select": "^5.7.0",
        "react-selectrix": "^1.0.17",
        "react-spinners": "^0.13.7",
        "styled-components": "^5.2.1",
        "styled-system": "^5.1.5",
        "use-debounce": "^9.0.2",
        "babel-plugin-styled-components": "^1.12.0",
        "yup": "^0.32.11",
        "yup-password": "^0.2.2"
    },
    "devDependencies": {
        "@svgr/cli": "^5.5.0",
        "@testing-library/react": "^11.2.3",
        "@testing-library/react-hooks": "^5.0.0",
        "@types/node": "^14.14.13",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "dotenv": "^8.2.0",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-prettier": "^3.2.0",
        "husky": "^4.3.7",
        "identity-obj-proxy": "^3.0.0",
        "isomorphic-unfetch": "^3.1.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "nock": "^13.0.6",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "npm run lint -- --quiet --fix"
        ]
    },
    "resolutions": {
        "@types/react": "^17.0.38"
    }
}
